{
  "name": "Muse-eXchange",
  "version": "1.0.0",
  "description": "This is the repository for our 3rd Project",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickgusd/Muse-eXchange.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickgusd/Muse-eXchange/issues"
  },
  "homepage": "https://github.com/nickgusd/Muse-eXchange#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "webpack-dynamic-public-path": "^1.0.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "client": "file:client",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.7",
    "react-calendly": "^1.1.1",
    "react-h5-audio-player": "^3.4.1",
    "react-motion": "^0.5.2",
    "react-multi-carousel": "^2.5.5",
    "react-router-dom": "^5.2.0",
    "react-swipeable-views": "^0.13.9"
  }
}
